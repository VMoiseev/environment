{
  "extends": "airbnb-base",
  "env": {
      "es2021": true,
      "browser": true,
      "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ]
  }
}